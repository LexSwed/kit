{
  "name": "@fxtrot/ui",
  "description": "UI library for Fxtrot initiatives",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "types": "./src/lib/index.ts",
  "main": "./src/lib/index.ts",
  "module": "./src/lib/index.ts",
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "import": "./src/lib/index.ts"
    },
    "./tailwind-preset": {
      "types": "./src/lib/tailwind/preset.ts",
      "import": "./src/lib/tailwind/preset.ts"
    },
    "./*": {
      "types": "./src/lib/*",
      "import": "./src/lib/*"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:lib && npm run build:types && npm run build:preset",
    "build:lib": "vite build --mode production",
    "build:types": "tsc --project tsconfig.lib.json",
    "build:preset": "vite build --config vite.tailwind-preset-config.mjs",
    "dev": "npm run docs:dev",
    "lint": "next lint --dir pages --dir src",
    "typecheck": "tsc --noEmit",
    "docs:dev": "next dev -p 3001",
    "docs:build": "next build",
    "docs:start": "next start"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,mdx,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-direction": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-id": "1.0.1",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-portal": "^1.0.3",
    "@radix-ui/react-presence": "^1.0.1",
    "@radix-ui/react-roving-focus": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@tw-classed/core": "^1.5.1",
    "clsx": "^1.2.1",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.6",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@stitches/react": "^1.2.8",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "gray-matter": "^4.0.3",
    "jest": "^29.5.0",
    "next": "^13.4.6",
    "next-seo": "^6.0.0",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^8.5.0",
    "react-icons": "^4.9.0",
    "react-live": "^4.1.3",
    "rehype-slug": "^5.1.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-code-meta": "^2.0.0",
    "remark-mdx-frontmatter": "^3.0.0",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.16",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "library",
    "design-system"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "quoteProps": "consistent"
  },
  "browserslist": [
    "> 2%"
  ],
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
